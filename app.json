{
  "expo": {
    "name": "caentrenos",
    "slug": "caentrenos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/apploading.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "../assets/apploading.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../assets/apploading.png",
        "backgroundColor": "#FFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
